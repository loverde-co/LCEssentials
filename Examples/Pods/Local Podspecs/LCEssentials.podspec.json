{
  "name": "LCEssentials",
  "version": "0.5.7",
  "summary": "This is a repository of essential scripts written in Swift for Loverde Co.",
  "description": "This is a repository of essential scripts written in Swift for Loverde Co. used to save time on re-writing and keeping it on all other projects. So this Cocoapods will evolve with Swift and will improve with every release!",
  "homepage": "https://github.com/loverde-co/LCEssentials",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Daniel Arantes Loverde": "daniel@loverde.com.br"
  },
  "source": {
    "git": "https://github.com/loverde-co/LCEssentials.git",
    "tag": "0.5.7"
  },
  "swift_versions": "5.0",
  "platforms": {
    "watchos": "5.0",
    "ios": "11.4"
  },
  "ios": {
    "source_files": [
      "Sources/LCEssentials/Classes/*.{swift}",
      "Sources/LCEssentials/PickerViews/*.{swift}",
      "Sources/LCEssentials/Notifications/*.{swift}",
      "Sources/LCEssentials/ImageZoom/*.{swift}",
      "Sources/LCEssentials/ImagePicker/*.{swift}",
      "Sources/LCEssentials/HUDAlert/*.{swift}"
    ],
    "resources": [
      "Sources/LCEssentials/PickerViews/Storyboards/*.{storyboard}",
      "Sources/LCEssentials/Notifications/Storyboard/*.{storyboard}",
      "Sources/LCEssentials/Notifications/Storyboard/*.{png}",
      "Sources/LCEssentials/ImageZoom/Storyboard/*.{storyboard}",
      "Sources/LCEssentials/ImagePicker/Storyboard/*.{storyboard}",
      "Sources/LCEssentials/HUDAlert/Storyboard/*.{storyboard}"
    ]
  },
  "watchos": {
    "source_files": "Sources/LCEssentials/Classes/*.{swift}"
  },
  "subspecs": [
    {
      "name": "Classes",
      "source_files": "Sources/LCEssentials/Classes/*.{swift}",
      "watchos": {
        "source_files": "Sources/LCEssentials/Classes/*.{swift}"
      }
    },
    {
      "name": "PickerViews",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/PickerViews/*.{swift}"
      ],
      "ios": {
        "resources": [
          "Sources/LCEssentials/PickerViews/Storyboards/*.{storyboard}"
        ]
      }
    },
    {
      "name": "Notifications",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/Notifications/*.{swift}"
      ],
      "ios": {
        "resources": [
          "Sources/LCEssentials/Notifications/Storyboards/*.{storyboard}",
          "Sources/LCEssentials/Notifications/Storyboard/*.{png}"
        ]
      }
    },
    {
      "name": "ImagesZoom",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/ImageZoom/*.{swift}"
      ],
      "ios": {
        "resources": [
          "Sources/LCEssentials/ImageZoom/Storyboards/*.{storyboard}"
        ]
      }
    },
    {
      "name": "ImagePicker",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/ImagePicker/*.{swift}"
      ],
      "ios": {
        "resources": [
          "Sources/LCEssentials/ImagePicker/Storyboards/*.{storyboard}"
        ]
      }
    },
    {
      "name": "HUDAlert",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/HUDAlert/*.{swift}"
      ],
      "ios": {
        "resources": [
          "Sources/LCEssentials/HUDAlert/Storyboards/*.{storyboard}"
        ]
      }
    }
  ],
  "swift_version": "5.0"
}
