{
  "name": "LCEssentials",
  "version": "0.8.0",
  "summary": "This is a repository of essential algorithm written in Swift for Loverde Co.",
  "description": "This is a repository of essential algorithm written in Swift for Loverde Co. used to save time on re-writing and keeping it on all other projects. So this Cocoapods will evolve with Swift and will improve with every release!",
  "homepage": "https://github.com/loverde-co/LCEssentials",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Daniel Arantes Loverde": "daniel@loverde.com.br"
  },
  "source": {
    "git": "https://github.com/loverde-co/LCEssentials.git",
    "tag": "0.8.0"
  },
  "swift_versions": "5.0",
  "platforms": {
    "ios": "15.0"
  },
  "ios": {
    "source_files": [
      "Sources/LCEssentials/Classes/*.{swift}",
      "Sources/LCEssentials/HUDAlert/*.{swift}",
      "Sources/LCEssentials/ImageZoom/*.{swift}",
      "Sources/LCEssentials/ImagePicker/*.{swift}"
    ]
  },
  "pod_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "user_target_xcconfig": {
    "EXCLUDED_ARCHS[sdk=iphonesimulator*]": "arm64"
  },
  "subspecs": [
    {
      "name": "Classes",
      "source_files": "Sources/LCEssentials/Classes/*.{swift}"
    },
    {
      "name": "ImageZoom",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/ImageZoom/*.{swift}"
      ]
    },
    {
      "name": "ImagePicker",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/ImagePicker/*.{swift}"
      ]
    },
    {
      "name": "HUDAlert",
      "source_files": [
        "Sources/LCEssentials/Classes/*.{swift}",
        "Sources/LCEssentials/HUDAlert/*.{swift}"
      ]
    }
  ],
  "swift_version": "5.0"
}
